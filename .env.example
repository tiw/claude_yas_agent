# 数据分析Agent配置文件示例

# 调试配置
DEBUG_MODE=true
LANGFUSE_ENABLED=false

# Langfuse配置（如果启用）
# LANGFUSE_PUBLIC_KEY=your_public_key
# LANGFUSE_SECRET_KEY=your_secret_key
# LANGFUSE_HOST=https://cloud.langfuse.com

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=agent.log

# 缓存配置
CACHE_ENABLED=true
CACHE_TTL=3600

